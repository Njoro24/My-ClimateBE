# Climate Witness Chain Backend Environment Variables

# Database
DATABASE_URL=sqlite:///./climate_witness.db

# API Keys
ASI_API_KEY=your-asi-api-key-here

# GPT-OSS-20B Configuration
GPT_OSS_ENABLED=true
GPT_OSS_BASE_URL=https://inference.asicloud.cudos.org/v1
GPT_OSS_MODEL=openai/gpt-oss-20b

# Blockchain Configuration
BLOCKCHAIN_ENABLED=true
POLYGON_RPC_URL=https://rpc-mumbai.maticvigil.com
CHAIN_ID=80001

# Security
JWT_SECRET=your-jwt-secret-here
ENCRYPTION_KEY=your-encryption-key-here

# External Services
WEATHER_API_KEY=your-weather-api-key
IPFS_GATEWAY=https://ipfs.io/ipfs/

# Development
DEBUG=true
LOG_LEVEL=INFO